      }
    },
    {
      "apply": {
        "model": "ftbic:block/hv_cable_connection",
        "x": 90,
        "y": 180
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "ftbic:block/hv_cable_connection",
        "x": 90
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "ftbic:block/hv_cable_connection",
        "x": 90,
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "ftbic:block/hv_cable_connection",
        "x": 90,
        "y": 270
      },
      "when": {
        "east": "true"
      }
    }
  ]
}